"*":
  "animated-page-scroll": {}
  "autocomplete-plus":
    includeCompletionsFromAllBuffers: false
  "autohide-tree-view":
    minWidth: 1
  core:
    disabledPackages: [
      "linter-scss-lint"
      "linter-stylelint"
      "autohide-tree-view"
      "linter-eslint"
      "linter-jscs"
    ]
    ignoredNames: [
      ".git"
      ".svn"
      ".DS_Store"
      "bower_components"
      "node_modules"
      "dist"
      "tmp"
      ".bower"
      ".sass-cache"
    ]
    themes: [
      "atom-dark-ui"
      "atom-dark-syntax"
    ]
  editor:
    invisibles: {}
    tabType: "soft"
  emmet: {}
  "exception-reporting":
    userId: "1fd414bd-4f7d-47c6-636e-cd290c31158b"
  "file-types":
    hbs: "text.html.htmlbars"
  linter:
    lintOnFly: false
  "linter-eslint": {}
  "linter-jscs":
    onlyConfig: true
  "linter-jshint":
    lintJSXFiles: true
  metrics:
    userId: "04e2b59bf2c9b8fcf465d9823a11c48db9b113a9"
  minimap:
    displayCodeHighlights: false
  "one-dark-ui": {}
  "release-notes":
    viewedVersion: "0.81.0"
  "tree-view":
    autoReveal: true
    hideIgnoredNames: true
  welcome:
    showOnStartup: false
"source.gfm":
  editor:
    softWrap: true
